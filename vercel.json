{
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1" }, 
      { "source": "/categories/(.*)", "destination": "/categories/$1" }, 
      { "source": "/blog/(.*)", "destination": "/blog/$1" },
      { "source": "/sitemap.xml", "destination": "/api/sitemap" },
      { "source": "/(.*)", "destination": "/" }
    ]
  }
  